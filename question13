numlist=[]
while True:
    num = int(input("enter three digit numbers:"))
    numlist.append(num)
    if num == 0:
        break
print("\n---square of three digit numbers is---")    
c = 0
for i in numlist:
    if i > 99 and i < 1000:
        squ = i * i
        print("\n->the square of", i ,"=", squ)
        c= c + 1
    
            
        
        
    


    

    
    
    
