numlist=[]
while True:
    num = int(input("enter three digit numbers:"))
    numlist.append(num)
    if num == 0:
        break
c = 0
for i in numlist:
    if  i < 100:
        n = i
        print(n)
    if i > 99 and i < 1000:
        squroot = i ** (0.5)
        print("\n->the square root of", i ,"=", squroot)
        c = c + 1
    if i > 999 :
        n = i
        print(n)
    
            
        
        
    


    

    
    
    
