numlist=[]
while True:
    num = int(input("enter three digit numbers:"))
    numlist.append(num)
    if num == 0:
        break
print("\n---square of three digit numbers is---")    
c = 0
for i in numlist:
    if i > 99 and i < 1000:
        squroot = i ** (0.5)
        print("\n->the square root of", i ,"=", squroot)
        c = c + 1
    
            
        
        
    


    

    
    
    
